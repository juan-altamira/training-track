{
	"name": "app",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "playwright test",
		"test:import": "tsx --test tests/import/*.test.ts",
		"test:smoke": "PW_JSON_REPORT=test-results/reports/smoke.json playwright test e2e/smoke --project=chromium",
		"test:smoke:mobile": "PW_JSON_REPORT=test-results/reports/smoke-mobile.json playwright test e2e/smoke --project=mobile-chromium",
		"test:full": "PW_JSON_REPORT=test-results/reports/full.json playwright test e2e/full --project=chromium --retries=1",
		"test:ui": "playwright test --ui",
		"test:headed": "playwright test --headed",
		"test:debug": "playwright test --debug",
		"test:report": "playwright show-report",
		"bench:panel": "tsx tests/benchmarks/panel-navigation.bench.ts",
		"bench:panel:mobile": "tsx tests/benchmarks/panel-navigation.mobile.bench.ts",
		"test:cleanup:verify": "tsx tests/helpers/sweep.ts verify-all",
		"audit:report": "tsx tests/reporting/build-audit-report.ts",
		"test:audit:nightly": "npm run test:full && npm run test:smoke:mobile && npm run bench:panel && npm run bench:panel:mobile && npm run test:cleanup:verify && npm run audit:report"
	},
	"devDependencies": {
		"@axe-core/playwright": "^4.10.2",
		"@browserbasehq/stagehand": "^3.0.7",
		"@playwright/test": "^1.57.0",
		"@sveltejs/adapter-vercel": "^6.3.0",
		"@sveltejs/kit": "^2.48.5",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"autoprefixer": "^10.4.22",
		"dotenv": "^16.6.1",
		"postcss": "^8.5.6",
		"svelte": "^5.43.8",
		"svelte-check": "^4.3.4",
		"tailwindcss": "^3.4.13",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"vite": "^7.2.2",
		"zod": "^4.3.5"
	},
	"dependencies": {
		"@supabase/auth-helpers-sveltekit": "^0.15.0",
		"@supabase/supabase-js": "^2.86.2",
		"@thednp/dommatrix": "^3.0.2",
		"mammoth": "^1.11.0",
		"pdfjs-dist": "^5.4.624",
		"xlsx": "^0.18.5"
	}
}
